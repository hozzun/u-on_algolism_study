# 시험장 수
N = int(input())
# 각 시험장 응시자 수
arr = list(map(int, input().split()))
# b = 총 감독관 감시 수, c = 부감독관 감시 수
# 총 감독관은 무조건 1명
b, c = map(int, input().split())

d = arr.pop(0) - b
arr.insert(0, d)

cnt = 1
for i in arr:
    while True:
        if i <= 0:
            break

        i -= c
        cnt += 1

print(cnt)
